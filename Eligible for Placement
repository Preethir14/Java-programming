import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        String n=s.next();
        int a=s.nextInt();
        int c=s.nextInt();
         System.out.println("Name of the Student:"+n);
        if(a==1 && c>70){
            System.out.println(n+" is Eligible for Placement");
        }
        else if(a==1 || a==2 && c>75){
            System.out.println(n+" is Eligible for Placement");
        }
        else{
            System.out.println(n+" is Not Eligible for Placement");
        }
    }
}
