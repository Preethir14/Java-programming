import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int a=s.nextInt();
        int b=s.nextInt();
        int c=s.nextInt();
        System.out.println("Number of copies sold:"+a);
        System.out.println("Cost of each copy:"+b);
        System.out.println("Cost spent by agency on each newspaper:"+c);
        float e=((a*b)-(a*c)-100);
        System.out.printf("The profit obtained is Rs.%.2f",e);
    }
}
